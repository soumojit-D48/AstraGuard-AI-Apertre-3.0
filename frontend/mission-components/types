export interface MissionState {
  name: string;
  phase: string;
  status: string;
  updated: string;
  anomalyCount: number;
}

export type TabType = 'mission' | 'systems';

export interface DashboardProps {
  data?: MissionState;
}

export interface NavItem {
  icon: string;
  label: string;
  href: string;
}
